{
    "name": "server",
    "version": "1.0.0",
    "scripts": {
        "start:dev": "cross-env NODE_ENV=development nodemon --watch . -r dotenv/config -r esm ./src/app.js",
        "start:prod": "cross-env NODE_ENV=production nodemon --watch . -r dotenv/config -r esm ./src/app.js",
        "start:babel": "nodemon --watch . -r dotenv/config --exec babel-node ./src/app.js",
        "build:.": "babel ./src -d ./lib --copy-files",
        ".:setup": "sequelize db:migrate:undo:all --env=production && sequelize db:migrate --env=production",
        "pretest:.": "npm run refresh:test",
        "test:.": "cross-env NODE_ENV=test TOKEN_PASSWORD=test nyc mocha ./test -t 40000 --exit --recursive -r dotenv/config -r esm",
        "test:babel": "cross-env NODE_ENV=test TOKEN_PASSWORD=test nyc mocha ./test --exit --recursive -r dotenv/config -r babel-polyfill -r babel-register",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "report": "nyc report --reporter=text-lcov",
        "lint": "eslint ./src/**/*.js --fix",
        "migrate": "sequelize db:migrate",
        "migrate:test": "sequelize db:migrate --env=test",
        "drop": "sequelize db:migrate:undo:all",
        "drop:test": "sequelize db:migrate:undo:all --env=test",
        "seed": "sequelize db:seed:all",
        "seed:test": "sequelize db:seed:all --env=test",
        "refresh": "npm-run-all drop migrate seed",
        "refresh:test": "npm-run-all drop:test migrate:test seed:test"
    },
    "devDependencies": {
        "autoprefixer": "^9.7.5",
        "axios-mock-adapter": "^1.18.1",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-add-module-exports": "^1.0.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.26.0",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "coveralls": "^3.0.11",
        "cross-env": "^6.0.3",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-plugin-import": "^2.20.1",
        "jest": "^24.9.0",
        "jest-enzyme": "^7.1.2",
        "mocha": "^6.2.2",
        "mockdate": "^2.0.5",
        "moxios": "^0.4.0",
        "nodemon": "^1.19.4",
        "npm-run-all": "^4.1.5",
        "nyc": "^14.1.1",
        "sequelize-cli": "^5.5.1",
        "sinon": "^7.5.0",
        "sinon-chai": "^3.5.0",
        "sinon-express-mock": "^2.2.1",
        "testdouble": "^3.13.1",
        "timemachine": "^0.3.1"
    },
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "ajv": "^6.12.0",
        "axios": "^0.19.2",
        "babel-polyfill": "^6.26.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "css-loader": "^3.4.2",
        "date-fns": "^2.11.0",
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "express-joi-validation": "^4.0.3",
        "jest-cli": "^24.9.0",
        "jsonwebtoken": "^8.5.1",
        "lodash.isequal": "^4.5.0",
        "pg": "^7.18.2",
        "pg-hstore": "^2.3.3",
        "sequelize": "^5.21.5",
        "swagger-ui-express": "^4.1.4",
        "uuid": "^3.4.0",
        "webpack": "^4.42.1",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-hot-middleware": "^2.25.0"
    }
}
