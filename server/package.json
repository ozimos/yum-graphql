{
    "name": "server",
    "version": "1.0.0",
    "scripts": {
        "start:dev": "dotenv -e .env -e .dev.env -- nodemon --inspect . -r esm ./src/app.js",
        "start:prod": "dotenv -e .env -e .prod.env -- nodemon --watch . -r esm ./src/app.js",
        "start:babel": "dotenv nodemon --watch . --exec babel-node ./src/app.js",
        "build:.": "babel ./src -d ./lib --copy-files",
        "server:setup": "sequelize db:migrate:undo:all --env=production && sequelize db:migrate --env=production",
        "test": "run-s refresh:test test:server",
        "debug": "dotenv -- nodemon -r esm --inspect ./src/app.js",
        "test:server": "dotenv -e .env -e .test.env -- nyc mocha ./test -t 40000 --exit --recursive -r esm",
        "test:babel": "dotenv -e .env -e .test.env -- nyc mocha ./test --exit --recursive",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "report": "nyc report --reporter=text-lcov",
        "lint": "eslint ./src/**/*.js --fix",
        "migrate": "sequelize db:migrate",
        "drop": "sequelize db:migrate:undo:all",
        "seed": "sequelize db:seed:all",
        "refresh": "run-s \"drop --env={1}\" \"migrate --env={1}\" --",
        "refresh:test": "yarn run refresh test"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.9.0",
        "autoprefixer": "^9.7.5",
        "axios-mock-adapter": "^1.18.1",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^25.2.4",
        "babel-loader": "^8.1.0",
        "babel-plugin-add-module-exports": "^1.0.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-register": "^6.26.0",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "chai-subset": "^1.6.0",
        "coveralls": "^3.0.11",
        "cross-env": "^7.0.2",
        "dotenv-cli": "^3.1.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-plugin-import": "^2.20.2",
        "faker": "^4.1.0",
        "jest": "^25.2.4",
        "jest-enzyme": "^7.1.2",
        "lodash": "^4.17.15",
        "mocha": "^7.1.1",
        "mockdate": "^2.0.5",
        "moxios": "^0.4.0",
        "nodemon": "^2.0.2",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.0.0",
        "sequelize-cli": "^5.5.1",
        "sinon": "^7.5.0",
        "sinon-chai": "^3.5.0",
        "sinon-express-mock": "^2.2.1",
        "testdouble": "^3.13.1",
        "timemachine": "^0.3.1",
        "typescript": "^3.8.3",
        "uuidv4": "^6.0.6"
    },
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "@yarnpkg/pnpify": "^2.0.0-rc.20",
        "ajv": "^6.12.0",
        "axios": "^0.19.2",
        "babel-polyfill": "^6.26.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "css-loader": "^3.4.2",
        "date-fns": "^2.11.1",
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "express-joi-validation": "^4.0.3",
        "jest-cli": "^25.2.4",
        "jsonwebtoken": "^8.5.1",
        "lodash.isequal": "^4.5.0",
        "pg": "^8.0.0",
        "pg-hstore": "^2.3.3",
        "sequelize": "^5.21.5",
        "swagger-ui-express": "^4.1.4",
        "uuid": "^7.0.2",
        "webpack": "^4.42.1",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-hot-middleware": "^2.25.0"
    }
}
